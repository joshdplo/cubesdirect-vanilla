{
  "name": "cubesdirect-vanilla",
  "version": "1.0.0",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "npm run dev",
    "dev": "concurrently --kill-others --names client,server \"vite build --watch\" \"nodemon server.js\"",
    "build": "vite build",
    "prod": "NODE_ENV=production node server.js",
    "seed-db": "node scripts/seed-db.js",
    "test-email": "node scripts/test-email.js",
    "json-structure": "npx directory-tree --path ./ --attributes type,extension,size -e \"node_modules|.git\" --pretty -o ./data/cubesdirect-structure.json"
  },
  "nodemonConfig": {
    "ext": "js,ejs",
    "ignore": [
      "node_modules/**",
      "src/**",
      "public/**"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "connect-session-sequelize": "^7.1.7",
    "cookie-parser": "^1.4.7",
    "dotenv": "^16.4.5",
    "ejs": "^3.1.10",
    "express": "^5.0.1",
    "express-session": "^1.18.1",
    "helmet": "^8.0.0",
    "joi": "^17.13.3",
    "jsonwebtoken": "^9.0.2",
    "nodemailer": "^6.9.15",
    "sequelize": "^6.37.4",
    "sqlite3": "^5.1.7",
    "uuid": "^10.0.0"
  },
  "devDependencies": {
    "@faker-js/faker": "^9.0.3",
    "concurrently": "^9.0.1",
    "nodemon": "^3.1.7",
    "vite": "^5.4.9"
  }
}